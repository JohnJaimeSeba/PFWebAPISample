@page "/"

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cute Homepage</title>
    <style>
        body {
            font-family: 'Comic Sans MS', cursive, sans-serif;
            text-align: center;
            background-color: #ffccdd;
            color: #333;
            padding: 20px;
        }
        h1 {
            font-size: 3em;
            color: #ff6699;
        }
        .container {
            background-color: white;
            padding: 20px;
            border-radius: 15px;
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);
            display: inline-block;
        }
        .cute-button {
            background-color: #ff6699;
            color: white;
            padding: 10px 20px;
            border: none;
            border-radius: 10px;
            font-size: 1.2em;
            cursor: pointer;
            transition: 0.3s;
        }
        .cute-button:hover {
            background-color: #ff3366;
        }
    </style>
</head>
<body>

    <div class="container">
        <h1>🌸Java-Wockezz🌸</h1>
        <p>Welcome to our System! Enjoy the cuteness🐱💖</p>
        <button class="cute-button">Click Me!</button>
    </div>

</body>


